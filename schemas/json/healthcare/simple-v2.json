{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
      "@context",
      "id",
      "type",
      "issuanceDate",
      "credentialSubject",
      "issuer"
    ],
    "properties": {
      "@context": {
        "type": ["string", "array", "object"]
      },
      "id": {
        "type": "string"
      },
      "type": {
        "type": ["string", "array"],
        "items": {
          "type": "string"
        }
      },
      "issuer": {
        "type": ["string", "object"],
        "format": "uri",
        "required": ["id"],
        "properties": {
          "id": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "issuanceDate": {
        "type": "string",
        "format": "date-time"
      },
      "credentialSubject": {
        "type": "object",
        "required": ["id", "testName"],
        "properties": {
          "id": {
            "title": "Credential Subject ID",
            "type": "string",
            "format": "uri"
          },
          "testName": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    }
  }
  